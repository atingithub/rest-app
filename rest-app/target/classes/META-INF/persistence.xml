<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="com.atin_rest-app_war_1.0-SNAPSHOTPU" transaction-type="RESOURCE_LOCAL">
    <non-jta-data-source>jdbc/restservice</non-jta-data-source>
    <class>com.atin.rest.model.Category</class>
    <class>com.atin.rest.model.Customer</class>
    <class>com.atin.rest.model.CustomerOrder</class>
    <class>com.atin.rest.model.OrderedProduct</class>
    <class>com.atin.rest.model.Product</class>
    <class>com.atin.rest.model.Admin</class>
    <class>com.atin.rest.model.User</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      <property name="eclipselink.logging.level" value="FINE"/>
    </properties>
  </persistence-unit>
</persistence>
